datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Lead {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  mobile    String   @unique
  postcode  String
  services  ServiceType
  createdAt DateTime @default(now())

}

enum ServiceType {
  DELIVERY
  PICKUP
  PAYMENT
}
